{
  "name": "@glideapps/vitest-v8-json-coverage-summary",
  "version": "0.0.0-echo",
  "description": "A plugin for vitest that generates a coverage summary in json format",
  "type": "module",
  "main": "index.js",
  "types": "dist/v8.json.summary.reporter.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./dist/v8.json.summary.reporter.d.ts"
    }
  },
  "files": [
    "dist/v8.json.summary.reporter.js",
    "dist/v8.json.summary.reporter.d.ts",
    "dist/v8.json.summary.reporter.js.map",
    "dist/v8.json.summary.reporter.d.ts.map",
    "dist/action.js",
    "dist/action.d.ts",
    "index.js",
    "action.yml",
    "example-workflow.yml",
    "README.md",
    "ACTION_README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vitest",
    "coverage",
    "reporter",
    "json",
    "v8",
    "testing",
    "github-actions"
  ],
  "author": "Typeguard Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/glideapps/vitest-v8-json-coverage-summary.git"
  },
  "bugs": {
    "url": "https://github.com/glideapps/vitest-v8-json-coverage-summary/issues"
  },
  "homepage": "https://github.com/glideapps/vitest-v8-json-coverage-summary#readme",
  "peerDependencies": {
    "vitest": ">=3.0.0"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}
